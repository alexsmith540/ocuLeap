{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["docs/index.md"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "githubRepoName": "vr.js",
    "githubRepoOwner": "benvanik"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "jsVersion": 180
}
